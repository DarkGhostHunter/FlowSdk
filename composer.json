{
  "name": "darkghosthunter/flow-sdk",
  "description": "Unofficial PHP SDK for Flow, a chilean payment gateway.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Italo Baeza C.",
      "email": "DarkGhostHunter@Gmail.com"
    }
  ],
  "support": {
    "source": "https://github.com/DarkGhostHunter/FlowSdk",
    "wiki": "https://github.com/DarkGhostHunter/FlowSdk/wiki",
    "docs": "https://www.flow.cl/docs/api.html",
    "email": "soporte@flow.cl"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1.3",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-openssl": "*",
    "psr/log": "^1.0.0",
    "guzzlehttp/guzzle": "6.3.* || 6.5.*",
    "katzgrau/klogger": "1.2.*",
    "darkghosthunter/fluid": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.0",
    "vlucas/phpdotenv": "2.5.* || 5.0.* || 5.2.* || 5.3.* || 5.4.*"
  },
  "autoload": {
    "psr-4": {
      "DarkGhostHunter\\FlowSdk\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  }
}
